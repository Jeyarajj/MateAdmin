select.round {
  background-image:
    linear-gradient(45deg, transparent 50%, gray 50%),
    linear-gradient(135deg, gray 50%, transparent 50%),
    radial-gradient(#ddd 70%, transparent 72%);
  background-position:
    calc(100% - 21px) calc(1em + 4px), calc(100% - 16px) calc(1em + 4px), calc(100% - 0.6em) 0.5em
  background-size:
    5px 5px,
    5px 5px,
    1.8em 1.8em;
  background-repeat: no-repeat;
}
  select.round:focus {
  background-image:
    linear-gradient(45deg, transparent 50%, #fafafa 50%),
    linear-gradient(135deg, #fafafa 50%, transparent 50%),
    radial-gradient(#607D8B 70%, transparent 72%);
  background-position:
    calc(100% - 21px) calc(1em + 4px), calc(100% - 16px) calc(1em + 4px), calc(100% - 0.6em) 0.5em
  background-size:
    5px 5px,
    5px 5px,
    1.8em 1.8em;
  background-repeat: no-repeat;
  }

